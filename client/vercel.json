{
  "version": 2,
  "name": "bug-tracker-client",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "@api_url"
  },
  "build": {
    "env": {
      "REACT_APP_API_URL": "@api_url"
    }
  }
}